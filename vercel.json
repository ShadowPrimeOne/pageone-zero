{
  "redirects": [
    {
      "source": "/",
      "destination": "/page/adwords-boost-au-electrician",
      "permanent": false
    },
    {
      "source": "/(.*)",
      "destination": "/page/adwords-boost-au-electrician",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs"
} 